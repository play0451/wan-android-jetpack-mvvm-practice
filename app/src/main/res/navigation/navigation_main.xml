        android:id="@+id/squareSystemSubFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.square.SquareSystemSubFragment"
        android:label="SquareSystemSubFragment" />
    <fragment
        android:id="@+id/shareAriticleFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.share.ShareAriticleFragment"
        android:label="ShareAriticleFragment" />
    <fragment
        android:id="@+id/userCoinFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.usercoin.UserCoinFragment"
        android:label="UserCoinFragment">
        <action
            android:id="@+id/action_userCoinFragment_to_userCoinHistoryFragment"
            app:destination="@id/userCoinHistoryFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/userCoinHistoryFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.usercoin.UserCoinHistoryFragment"
        android:label="UserCoinHistoryFragment" />
    <fragment
        android:id="@+id/collectFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.collect.CollectFragment"
        android:label="CollectFragment" />
    <fragment
        android:id="@+id/myShareAriticleFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.share.MyShareAriticleFragment"
        android:label="MyShareAriticleFragment">
        <action
            android:id="@+id/action_myShareAriticleFragment_to_shareAriticleFragment"
            app:destination="@id/shareAriticleFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/todoListFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.todo.TodoListFragment"
        android:label="TodoListFragment">
        <action
            android:id="@+id/action_todoListFragment_to_todoEditFragment"
            app:destination="@id/todoEditFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/todoEditFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.todo.TodoEditFragment"
        android:label="TodoEditFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/projectFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.project.ProjectFragment"
        android:label="ProjectFragment" />
    <fragment
        android:id="@+id/squareAriticleFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.square.SquareAriticleFragment"
        android:label="SquareAriticleFragment">
        <action
            android:id="@+id/action_squareAriticleFragment_to_shareAriticleFragment"
            app:destination="@id/shareAriticleFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/officialFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.official.OfficialFragment"
        android:label="OfficialFragment" />
    <fragment
        android:id="@+id/squareSystemFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.square.SquareSystemFragment"
        android:label="SquareSystemFragment">
        <action
            android:id="@+id/action_squareSystemFragment_to_squareSystemSubFragment"
            app:destination="@id/squareSystemSubFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/squareNavigationFragment"
        android:name="com.gw.mvvm.wan.ui.fragment.square.SquareNavigationFragment"
        android:label="SquareNavigationFragment" />
</navigation>