<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_2D2D2D</item>
        <item name="colorPrimaryVariant">@color/grey_900</item>
        <item name="colorOnPrimary">@color/grey_300</item>
        <item name="colorOnBackground">@color/grey_300</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cyan_200</item>
        <item name="colorSecondaryVariant">@color/cyan_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorTag">@color/grey_500</item>
        <item name="colorNavButtonSelected">@color/grey_300</item>
        <item name="colorNavButtonUnselected">@color/grey_600</item>
        <item name="colorItemDecoration">@color/grey_700</item>
        <item name="colorTodoItemDoneForeground">@color/color_80212121</item>
        <item name="colorDialogButton">@color/grey_300</item>
        <item name="colorCoinText">@color/grey_300</item>
        <item name="colorCollectButton">@color/white</item>
    </style>
</resources>